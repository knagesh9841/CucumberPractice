<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SeleniumTesting">

	<listeners>
		<listener class-name="com.crm.qa.listeners.SuiteListener"/>
        <listener class-name="com.crm.qa.listeners.TestListener"/>
        <listener class-name="com.crm.qa.listeners.InvokedMethodListener"/>
    </listeners>
    
    
<test name="EdgetestngTest">
 <parameter name="browser" value="edge"/>
 <classes>
 	<class name="com.crm.qa.testRunner.LoginTestNgRunner" />
</classes>
</test>    

    
<test name="FirefoxtestngTest">
 <parameter name="browser" value="firefox"/>
 <classes>
 	<class name="com.crm.qa.testRunner.TooltipDownloadTestNgRunner" />
</classes>
</test>
  
 

 
</suite>